{
  "name": "eleven",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:js && yarn lint:style && yarn lint:prettier",
    "lintfix": "prettier --write --list-different . && yarn lint:js --fix && yarn lint:style --fix",
    "knip": "knip"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint --cache",
    "*.{css,scss,sass,html,vue}": "stylelint",
    "*.**": "prettier --check --ignore-unknown"
  },
  "dependencies": {
    "@nuxt/image": "1.6.0",
    "@nuxt/ui": "^2.15.2",
    "@vueuse/core": "^10.9.0",
    "@vueuse/nuxt": "^10.9.0",
    "autoprefixer": "^10.4.19",
    "dompurify": "^3.1.1",
    "marked": "^12.0.2",
    "nuxt-icon": "^0.6.9",
    "nuxt-purgecss": "^2.0.0",
    "postcss": "^8.4.38",
    "posthog-js": "^1.130.0",
    "prettier": "^3.2.5"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.2.0",
    "@nuxtjs/i18n": "^8.3.1",
    "@nuxtjs/seo": "^2.0.0-rc.10",
    "@nuxtjs/sitemap": "^5.1.4",
    "@nuxtjs/strapi": "^1.12.0",
    "@pinia/nuxt": "^0.5.1",
    "@splidejs/vue-splide": "^0.6.12",
    "@types/node": "^20.12.10",
    "@unlok-co/nuxt-stripe": "^2.0.0",
    "@vueuse/components": "^10.8.0",
    "knip": "^5.13.0",
    "nuxt": "^3.11.2",
    "pinia": "^2.1.6",
    "typescript": "^5.4.5"
  },
  "overrides": {
    "vue": "latest"
  },
  "engines": {
    "node": "20.x"
  }
}
